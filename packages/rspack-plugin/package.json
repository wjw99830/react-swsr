{
  "name": "@react-swsr/rspack-plugin",
  "version": "0.0.2",
  "description": "Build swsr bundle for react swsr",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "swsr",
    "service-worker"
  ],
  "files": [
    "dist"
  ],
  "author": "wmh",
  "license": "MIT",
  "scripts": {
    "build": "tsup && cp -f src/entry.template.jsx dist",
    "version": "git add -A && git commit -m \"chore(rspack-plugin): release\" && pnpm build && pnpm publish --access public --registry https://registry.npmjs.org"
  },
  "dependencies": {
    "@rspack/core": "1",
    "cheerio": "1",
    "lodash": "4",
    "rslog": "1",
    "webpack-merge": ">= 4"
  },
  "devDependencies": {
    "@rspack/lite-tapable": "^1.0.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "16",
    "type-fest": "^4.26.1"
  },
  "engines": {
    "node": ">= 16"
  }
}
